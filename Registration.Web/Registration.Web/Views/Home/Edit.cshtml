                    <div class="col-md-1 text-right">
                        <label asp-for="Warranty" class="col-form-label">保固年份</label>
                    </div>
                    <div class="col-md-3">
                        <input asp-for="Warranty" class="form-control" />
                        <span asp-validation-for="Warranty" class="text-danger"></span>
                    </div>

                    <div class="col-md-1 text-right">
                        <label asp-for="WarrantyPeriodStr" class="col-form-label">保固期限</label>
                    </div>
                    <div class="col-md-2">
                        <input asp-for="WarrantyPeriodStr" class="form-control" type="date" />
                        <span asp-validation-for="WarrantyPeriodStr" class="text-danger"></span>
                    </div>
                    <div class="">~</div>
                    <div class="col-md-2">
                        <input asp-for="WarrantyPeriodEnd" class="form-control" type="date" />
                        <span asp-validation-for="WarrantyPeriodEnd" class="text-danger"></span>
                    </div>
                </div>

