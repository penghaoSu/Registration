                    <div class="col-md-1 text-right">
                        <label asp-for="Lease" class="col-form-label">租用期數</label>
                    </div>
                    <div class="col-md-3">
                        <input asp-for="Lease" class="form-control" />
                        <span asp-validation-for="Lease" class="text-danger"></span>
                    </div>

                    <div class="col-md-1 text-right">
                        <label asp-for="LeaseDateStr" class="col-form-label">租用期限</label>
                    </div>
                    <div class="col-md-2">
                        <input asp-for="LeaseDateStr" class="form-control" type="date" />
                        <span asp-validation-for="LeaseDateStr" class="text-danger"></span>
                    </div>
                    <div class="">~</div>
                    <div class="col-md-2">
                        <input asp-for="LeaseDateEnd" class="form-control" type="date" />
                        <span asp-validation-for="LeaseDateEnd" class="text-danger"></span>
                    </div>
                </div>

                <div class="form-group row">
                    <div class="col-md-1 text-right">
                        <label asp-for="Module" class="col-form-label">擴充模組</label>
                    </div>
                    <div class="col-md-3">
                        <select asp-for="Module" class="form-control mb-2 mr-sm-2 mb-sm-2">
                            <option value="1">庫存</option>
                            <option value="2">配方</option>
                            <option value="3">促銷</option>
                            <option value="4">APP</option>
                        </select>
                        <span asp-validation-for="Module" class="text-danger"></span>
                    </div>

                    <div class="col-md-1 text-right">
                        <label asp-for="IsAutoUpdate" class="col-form-label">自動更版</label>
                    </div>
                    <div class="col-md-3" style="padding-top:7px;">
                        <label class="custom-control custom-checkbox">
                            <input asp-for="IsAutoUpdate" type="checkbox" class="custom-control-input">
                            <span class="custom-control-indicator"></span>
                            <span class="custom-control-description"></span>
                        </label>
                    </div>

                </div>
            </div>
        </div>
        <br />

        <div class="row justify-content-center">
            <div class="col-4 text-center">
                <button type="submit" value="Create" class="btn btn-primary" id="Send">儲存<i class="fa fa-check"></i></button>
                <a asp-action="Index" class="btn btn-success">返回列表 <i class="fa fa-reply"></i></a>
            </div>
        </div>
    </form>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
